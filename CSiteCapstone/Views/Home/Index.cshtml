﻿@model System.Data.DataTable
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table border="1">
    <tr>
        <th>User Id</th>
        <th>User First Name</th>
        <th>User Last Name</th>
        <th>User Email</th>
        <th>User Password</th>
        <th>User Contact</th>
        <th>User Usertype</th>
        <th>User Created</th>


    </tr>

    @{
        for (int i = 0; i < Model.Rows.Count; i++)
        {
            <tr>
                <th>@Model.Rows[i][0]</th>
                <th>@Model.Rows[i][1]</th>
                <th>@Model.Rows[i][2]</th>
                <th>@Model.Rows[i][3]</th>
                <th>@Model.Rows[i][4]</th>
                <th>@Model.Rows[i][5]</th>
                <th>@Model.Rows[i][6]</th>
                <th>@Model.Rows[i][7]</th>

            </tr>
        }
    }


</table>

